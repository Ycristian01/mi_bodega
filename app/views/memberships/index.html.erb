<h1>Members</h1>
<table class="table table-bordered">
  <thead>
    <tr>
      <th scope="col">Email</th>
      <%= "<th></th>".html_safe if current_user.account&.id == current_tenant.id %>
    </tr>
  </thead>
  <tbody>
    <% @members.each do |member| %>
      <% if member.user.active_for_authentication? %>
        <tr>
          <td><%= member.user.email %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>